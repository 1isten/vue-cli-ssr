{
  "name": "vue-cli-ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node -r dotenv/config app.js",
    "serve": "vue-cli-service serve",
    "prebuild": "rimraf public/{js,css,fonts,img}",
    "build": "npm run build:client && npm run build:server",
    "postbuild": "rimraf public/index.html",
    "build:client": "cross-env TARGET_ENV=client vue-cli-service build --no-clean",
    "build:server": "cross-env TARGET_ENV=server vue-cli-service build --no-clean",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.4.0",
    "vuex-router-sync": "^5.0.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.1",
    "@vue/cli-plugin-eslint": "^4.4.1",
    "@vue/cli-plugin-router": "^4.4.1",
    "@vue/cli-plugin-vuex": "^4.4.1",
    "@vue/cli-service": "^4.4.1",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-vue": "^6.2.2",
    "rimraf": "^3.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}
